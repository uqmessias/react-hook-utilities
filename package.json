{
  "name": "react-hook-utilities",
  "version": "0.1.0",
  "description": "Utilitarian React hooks",
  "author": "Fl√°vio Caetano (@fjcaetano)",
  "license": "MIT",
  "keywords": [
    "react",
    "hook",
    "hoc",
    "compose",
    "react-native",
    "recompose"
  ],
  "homepage": "https://github.com/fjcaetano/react-hook-utilities.git",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/fjcaetano/react-hook-utilities.git"
  },
  "scripts": {
    "build": "tsc && prettier --check 'dist/**/*' --write",
    "test": "jest --coverage",
    "lint": "eslint --ext=.ts,.tsx --max-warnings=0 src/ && prettier --check 'src/**/*'",
    "docs": "typedoc --out docs/$npm_package_version && scripts/gen-docs-index.sh $npm_package_version"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "eslint-plugin/!(*/__tests__|yarn*)"
  ],
  "dependencies": {
    "react": "^16"
  },
  "devDependencies": {
    "@react-hook-utilities/eslint-plugin": "./eslint-plugin",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "babel-eslint": "^10.0.3",
    "commitlint": "^8.2.0",
    "eslint": "^6.5.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.1.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.9.0",
    "ts-jest": "^24.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  }
